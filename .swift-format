{
  "indentation": {
    "spaces": 4
  },
  "lineLength": 100,
  "useSingleLinePropertyGetters": false,
  "fileScopedDeclarationPrivacy": {
    "accessLevel": "private"
  },
  "respectsExistingLineBreaks": true,
  "rules": {
    "AllPublicDeclarationsHaveDocumentation": false,
    "AlwaysUseLowerCamelCase": true,
    "AmbiguousTrailingClosureOverload": true,
    "BeginDocumentationComments": true,
    "BlankLinesBetweenImports": false,
    "CaseIndentation": true,
    "ConditionalCompilationClauseIndentation": true,
    "ConsecutiveDeclarations": true,
    "DontRepeatTypeInStaticProperties": true,
    "FullyIndentedQuotedStringLiterals": true,
    "GroupNumericLiterals": true,
    "IdentifiersMustBeASCII": true,
    "ImplicitlyUnwrappedOptionals": true,
    "IndentationWidth": true,
    "InvalidDeprecatedSymbol": true,
    "MissingLocalizationComment": true,
    "MultiLineStringIndentation": true,
    "NeverForceUnwrap": true,
    "NeverUseForceTry": true,
    "NeverUseImplicitlyUnwrappedOptionals": true,
    "NoAccessLevelOnExtensionDeclaration": true,
    "NoAssignmentInExpressions": true,
    "NoBlockComments": true,
    "NoCasesWithOnlyFallthrough": true,
    "NoEmptyTrailingClosureParentheses": true,
    "NoExtensionAccessModifier": true,
    "NoLabelsInCasePatterns": true,
    "NoLeadingUnderscores": false,
    "NoParensAroundConditions": true,
    "NoPlaygroundLiterals": true,
    "NoVoidReturnOnFunctionSignature": true,
    "OmitExplicitReturns": true,
    "OneVariableDeclarationPerLine": true,
    "OnlyOneTrailingClosureArgument": true,
    "OrderedImports": true,
    "Paren": true,
    "ReplaceForEachWithForLoop": true,
    "ReturnVoidInsteadOfEmptyParens": true,
    "SelfBindingVariableName": true,
    "SemicolonNotAllowed": true,
    "SingleLineIfStatements": true,
    "SortedImports": true,
    "SpaceAroundOperators": true,
    "SpacesInsideAndOutsideParentheses": false,
    "TrailingCommas": true,
    "TrailingWhitespace": true,
    "TypeNamesShouldBeCapitalized": true,
    "UnusedImports": true,
    "UseEarlyExits": true,
    "UseExplicitTypes": false,
    "UseSortedImports": true,
    "UseSpecificNumberRadix": true,
    "UseTripleSlashForDocumentationComments": true,
    "ValidateDocumentationComments": false,
    "VoidReturnTypeShouldNotBeIncluded": true
  }
}
